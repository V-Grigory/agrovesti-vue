  "description": "App run by nodeJS on backend (Express, ejs) and vueJs on frontend",
  "private": true,
  "scripts": {
    "buildProdFront": "cross-env PORT=3000 NODE_ENV=production webpack --progress",
    "startProd": "cross-env PORT=8081 NODE_ENV=production node ./bin/www",
    "startDev": "cross-env PORT=8083 NODE_ENV=production node ./bin/www",
    "buildDevFront": "cross-env PORT=3000 NODE_ENV=development webpack --progress",
    "dev": "cross-env PORT=8083 NODE_ENV=development nodemon ./bin/www"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/V-Grigory/agrovesti.ru.git"
