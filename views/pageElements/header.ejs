<div id="header" class="header">
    <div class="contentWrapper">
        <div class="mainMenu">

            <div class="leftSide">
                <a href="/" class="logo">
                    <img id="imgLogo" src="/images/logo.png" alt="agrovesti">
                </a>
            </div>

            <% if (pageData.menuData.rubriks.main_menu.length) { %>
                <div class="rightSide">

                    <div class="closeButtonMobileMainMenu">
                        &#10799;
                    </div>

                    <% pageData.menuData.rubriks.main_menu.forEach(menuItem => { %>
                      <a
                        href="/rubrics/<%= menuItem.name_en %>"
                        class="menuItem"
                      >
                        <%= menuItem.name_ru %>
                      </a>
                    <% }); %>
                    <a class="menuItem search" href="/search">
                        &#9906;
                    </a>
                </div>
            <% } %>

        </div>
    </div>
</div>
